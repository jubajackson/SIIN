<script language="javascript" type="text/javascript">
    $(document).ready(function () {
        getAddresses();
    });

    var _isStateSuppressed = false;
    var _isPostalCodeSuppressed = false;
    var _selectedAddressID = 0;

    function addNewShippingLocation() {
        clearErrors();

        showWaitDialog('Please Wait...');

        ddlShippingLocations.selectedIndex = 0;

        resetShippingLocationForm();

        hideWaitDialog();
    }

    function hideShippingLocationForm() {
        clearErrors();
        resetShippingLocationForm();
    }

    function onSelectedAddressChange() {
        showWaitDialog('Retrieving Address...');

        clearErrors();

        resetShippingLocationForm();

        _selectedAddressID = $("#ddlShippingLocations").val();

        if (_selectedAddressID > 0) {
            var parameters = { selectedAddressID: _selectedAddressID };

            $.ajax({
                type: "GET",
                url: '@Url.Action("GetSelectedAddressInformation", "Account")',
                contentType: 'application/json; charset=utf-8;',
                data: parameters,
                dataType: 'json',
                cache: false,
                success: function (address) {
                    if (!isNullOrUndefinedOrEmpty(address)) {
                        onSelectedAddressChangeComplete(address);
                    }
                    else {
                        addError("Unable to retrieve the address information.");
                        showErrors();
                        hideWaitDialog();
                    }
                },
                error: function (xhr, statusText, thrownError) {
                    addError("Unable to retrieve the address information.");
                    showErrors();
                    hideWaitDialog();
                }
            });
        }
        else {
            clearErrors();
            resetShippingLocationForm();

            hideWaitDialog();
        }
    }

    function onSelectedAddressChangeComplete(address) {
        $('#txtName').val(address.Name);
        $('#txtAddress1').val(address.Address1);
        $('#txtAddress2').val(address.Address2);
        $('#txtAddress3').val(address.Address3);
        $('#txtCity').val(address.City);
        $('#txtProvince').val(address.Province);
        $('#txtPostalCode').val(address.PostalCode);
        $('#txtPhoneNumber').val(address.Phone);

        $("#ddlCountry").val(address.Country);
        $("#ddlState").val(address.State);

        if (address.State != '' && address.State != undefined)
            $('#liState').show();

        if (address.Province != '' && address.Province != undefined) {
            $('#liProvince').show();
            $('#liState').hide();
        }

        if (address.PostalCode != '' && address.PostalCode != undefined) {
            $('#liPostalCode').show();
        }

        hideWaitDialog();
    }

    function resetShippingLocationForm() {
        $('#txtName').val('');
        $('#txtAddress1').val('');
        $('#txtAddress2').val('');
        $('#txtAddress3').val('');
        $('#txtCity').val('');
        $('#txtProvince').val('');
        $('#txtPostalCode').val('');
        $('#txtPhoneNumber').val('');

//        ddlCountry.selectedIndex = 0;
        ddlState.selectedIndex = 0;

        $('#liProvince').hide();
        $('#liState').show();
        $('#liPostalCode').show();
    }

    function clearErrors() {
        $("#txtPassword").removeClass("error");
        $("#txtNewPassword").removeClass("error");
        $("#txtConfirmNewPassword").removeClass("error");
        $("#ddlCountry").removeClass("error");
        $("#ddlState").removeClass("error");
        $("#txtName").removeClass("error");
        $("#txtAddress1").removeClass("error");
        $("#txtAddress2").removeClass("error");
        $("#txtAddress3").removeClass("error");
        $("#txtCity").removeClass("error");
        $("#txtProvince").removeClass("error");
        $("#txtPostalCode").removeClass("error");
        $("#txtPhoneNumber").removeClass("error");

        hideErrors();
        hideMessages();
    }

    function getAddressFormat() {
        showWaitDialog('Retrieving Address Format...');

        if (ddlCountry.selectedIndex < 1) {
            addError("The Country field is required.");
            $("#ddlCountry").addClass("error");

            showErrors();
        }
        else {
            var selectedCountry = $("#ddlCountry").val();

            var parameters = { countryCode: selectedCountry };

            $.ajax({
                type: "GET",
                url: '@Url.Action("GetAddressFormat", "Account")',
                contentType: 'application/json; charset=utf-8;',
                data: parameters,
                dataType: 'json',
                cache: false,
                success: function (addressFormat) {
                    if (!isNullOrUndefinedOrEmpty(addressFormat)) {
                        onGetAddressFormatComplete(addressFormat);
                    }
                    else {
                        addError("Unable to retrieve the address format for the selected country.");
                        showErrors();
                        hideWaitDialog();
                    }
                },
                error: function (xhr, statusText, thrownError) {
                    addError("Unable to retrieve the address format for the selected country.");
                    showErrors();
                    hideWaitDialog();
                }
            });
        }
    }

    function onGetAddressFormatComplete(addressFormat) {
        _isStateSuppressed = addressFormat.isStateSuppressed;
        _isPostalCodeSuppressed = addressFormat.isPostalCodeSuppressed;

        if (_isStateSuppressed) {
            ddlState.selectedIndex = 0;
            $("#liState").hide();
            $("#liProvince").hide();
        }
        else {
            if ($("#ddlCountry").val() == "US") {
                $("#liState").show();
            }
            else {
                $("#liProvince").show();
            }
        }

        if (_isPostalCodeSuppressed) {
            $("#txtPostalCode").val('');
            $("#liPostalCode").hide();
        }
        else {
            $("#liPostalCode").show();
        }

        hideWaitDialog();
    }

    function saveShippingLocation() {
        showWaitDialog('Saving shipping location...');

        clearErrors();

        if (validateShippingLocation()) {
            var parameters = {
                addressID: _selectedAddressID,
                country: $("#ddlCountry").val(),
                name: $("#txtName").val(),
                address1: $("#txtAddress1").val(),
                address2: $("#txtAddress2").val(),
                address3: $("#txtAddress3").val(),
                city: $("#txtCity").val(),
                state: $("#ddlState").val(),
                province: $("#txtProvince").val(),
                postalCode: $("#txtPostalCode").val(),
                phoneNumber: $("#txtPhoneNumber").val()
            };

            $.ajax({
                type: "GET",
                url: '@Url.Action("SaveShippingLocation", "Account")',
                contentType: 'application/json; charset=utf-8;',
                data: parameters,
                dataType: 'json',
                cache: false,
                success: function (saved) {
                    if (saved) {
                        onSaveShippingLocationComplete();
                    }
                    else {
                        addError("The shipping location was not saved.");
                        showErrors();
                        hideWaitDialog();
                    }
                },
                error: function (xhr, statusText, thrownError) {
                    addError("The shipping location was not saved.");
                    showErrors();
                    hideWaitDialog();
                }
            });
        }
    }

    function onSaveShippingLocationComplete() {
        clearErrors();

        hideShippingLocationForm();

        addMessage("The shipping location was saved.");
        showMessages();

        getAddresses();
    }

    function validateShippingLocation() {
        var valid = true;

        clearErrors();

        var countryCode = $("#ddlCountry").val();

        if ($('#txtName').val().length <= 0) {
            addError("The Name field is required.");
            $("#txtName").addClass("error");

            valid = false;
        }

        if ($('#txtName').val().length > 25) {
            addError("The Name field can only be 25 characters long.");
            $("#txtName").addClass("error");

            valid = false;
        }

        if ($('#txtAddress1').val().length <= 0) {
            addError("The Address1 field is required.");
            $("#txtAddress1").addClass("error");

            valid = false;
        }

        if ($('#txtAddress1').val().length > 40) {
            addError("The Address1 field can only be 40 characters long.  You can use all three address fields to enter your address.");
            $("#txtAddress1").addClass("error");

            valid = false;
        }

        if ($('#txtAddress2').val().length > 40) {
            addError("The Address2 field can only be 40 characters long.");
            $("#txtAddress2").addClass("error");

            valid = false;
        }

        if ($('#txtCity').val().length <= 0) {
            addError("The City field is required.");
            $("#txtCity").addClass("error");

            valid = false;
        }

        if ($('#txtCity').val().length > 25) {
            addError("The City field can only be 25 characters long.");
            $("#txtCity").addClass("error");

            valid = false;
        }

        if (!_isStateSuppressed) {
            if (countryCode == "US") {
                if (ddlState.selectedIndex < 1) {
                    addError("The State field is required.");
                    $("#ddlState").addClass("error");

                    valid = false;
                }
            }
            else {
                if ($('#txtProvince').val().length <= 0) {
                    addError("The Province field is required.");
                    $("#txtProvince").addClass("error");

                    valid = false;
                }

                if ($('#txtProvince').val().length > 3) {
                    addError("Please enter your province code.  The maximum amount of characters is 3.");
                    $("#txtProvince").addClass("error");

                    valid = false;
                }
            }
        }

        if (!_isPostalCodeSuppressed) {
            if ($('#txtPostalCode').val().length <= 0) {
                addError("The Postal Code field is required.");
                $("#txtPostalCode").addClass("error");

                valid = false;
            }
        }

        if ($('#txtPostalCode').val().length > 25) {
            addError("The Postal Code field can only be 10 characters long.");
            $("#txtPostalCode").addClass("error");

            valid = false;
        }

        if (!valid) {
            showErrors();
        }

        return valid;
    }

    function deleteShippingLocation() {
        clearErrors();
        resetShippingLocationForm();
        showWaitDialog('Deleting Shipping Location...');

        if (confirm('Are you sure you wish to remove this shipping location? This action cannot be reversed.')) {
            var parameters = { selectedAddressID: _selectedAddressID };

            $.ajax({
                type: "GET",
                url: '@Url.Action("DeleteShippingLocation", "Account")',
                contentType: 'application/json; charset=utf-8;',
                data: parameters,
                dataType: 'json',
                cache: false,
                success: function (deleted) {
                    if (deleted) {
                        onDeleteShippingLocationComplete();
                    }
                    else {
                        addError("The shipping location was not deleted.");
                        showErrors();
                        hideWaitDialog();
                    }
                },
                error: function (xhr, statusText, thrownError) {
                    addError("The shipping location was not deleted.");
                    showErrors();
                    hideWaitDialog();
                }
            });
        }
    }

    function onDeleteShippingLocationComplete() {
        hideShippingLocationForm();

        addMessage('The shipping location has been deleted.');
        showMessages();

        getAddresses();
    }

    function cancelAddEditShippingAddress() {
        hideShippingLocationForm();
    }

    function changePassword() {
        clearErrors();

        if (validatePasswordChange()) {
            showWaitDialog('Changing your password...');

            var parameters = {
                oldPassword: $('#txtPassword').val(),
                newPassword: $('#txtNewPassword').val()
            };

            $.ajax({
                type: "GET",
                url: '@Url.Action("ChangePassword", "Account")',
                contentType: 'application/json; charset=utf-8;',
                data: parameters,
                dataType: 'json',
                cache: false,
                success: function (saved) {
                    if (saved) {
                        onChangePasswordComplete();
                    }
                    else {
                        addError("Your password was not changed.");
                        showErrors();
                        hideWaitDialog();
                    }
                },
                error: function (xhr, statusText, thrownError) {
                    addError("Your password was not changed.");
                    showErrors();
                    hideWaitDialog();
                }
            });
        }
    }

    function onChangePasswordComplete() {
        resetChangePasswordForm();

        addMessage("Your password was changed.");
        showMessages();

        hideWaitDialog();
    }

    function validatePasswordChange() {
        var valid = true;

        if ($('#txtPassword').val().length <= 0) {
            addError("The Current Password field is required.");
            $("#txtPassword").addClass("error");

            valid = false;
        }

        if ($('#txtNewPassword').val().length <= 0) {
            addError("The New Password field is required.");
            $("#txtNewPassword").addClass("error");

            valid = false;
        }

        if ($('#txtConfirmNewPassword').val().length <= 0) {
            addError("The Confirm New Password field is required.");
            $("#txtConfirmNewPassword").addClass("error");

            valid = false;
        }

        if ($('#txtNewPassword').val() != $('#txtConfirmNewPassword').val()) {
            addError("The new passwords do not match.");
            $("#txtNewPassword").addClass("error");
            $("#txtConfirmNewPassword").addClass("error");

            valid = false;
        }

        if (!valid) {
            showErrors();
        }

        return valid;
    }

    function cancelChangePassword() {
        resetChangePasswordForm();
    }

    function resetChangePasswordForm() {
        $("#txtPassword").val('');
        $("#txtNewPassword").val('');
        $("#txtConfirmNewPassword").val('');
    }

    function getAddresses() {
        $.ajax({
            type: "GET",
            url: '@Url.Action("GetAddresses", "Account")',
            contentType: 'application/json; charset=utf-8;',
            dataType: 'json',
            cache: false,
            success: function (addressList) {
                if (!isNullOrUndefinedOrEmpty(addressList)) {
                    onGetAddressesComplete(addressList);
                }
                else {
                    addMessage("You currently do not have any addresses stored in the system.");
                    showMessages();
                    hideWaitDialog();
                }
            },
            error: function (xhr, statusText, thrownError) {
                addError("An error occoured retrieving addresses.");
                showErrors();
                hideWaitDialog();
            }
        });
    }

    function onGetAddressesComplete(addressList) {
        $('#ddlShippingLocations').empty();

        addDropDownOption(document.getElementById("ddlShippingLocations"), "Select a Shipping Location to edit...", "0", false, false);

        for (var i = 0; i < addressList.length; i++) {
            var displayAddress = addressList[i].Name + ", " + addressList[i].City + ", " + addressList[i].State + ", " + addressList[i].PostalCode;

            addDropDownOption(document.getElementById("ddlShippingLocations"), displayAddress, addressList[i].Id, false, false);
        }

        hideWaitDialog();
    }
